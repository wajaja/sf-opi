<h1>Subcriber list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Post</th>
            <th>Subcribers</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for document in documents %}
        <tr>
            <td><a href="{{ path('subcriber_show', { 'id': document.id }) }}">{{ document.id }}</a></td>
            <td>{{ document.post }}</td>
            <td>{{ document.subcribers }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('subcriber_show', { 'id': document.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('subcriber_edit', { 'id': document.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('subcriber_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
