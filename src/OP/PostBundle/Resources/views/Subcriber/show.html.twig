<h1>Subcriber</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ document.id }}</td>
        </tr>
        <tr>
            <th>Post</th>
            <td>{{ document.post }}</td>
        </tr>
        <tr>
            <th>Subcribers</th>
            <td>{{ document.subcribers }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('subcriber') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('subcriber_edit', { 'id': document.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('subcriber_delete', { 'id': document.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
